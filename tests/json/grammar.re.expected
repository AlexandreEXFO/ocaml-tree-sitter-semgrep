type document =
  _value

and _value =
  [
  | `Case0(
      object_
    )
  | `Case1(
      array
    )
  | `Case2(
      number
    )
  | `Case3(
      string
    )
  | `Case4(
      true_
    )
  | `Case5(
      false_
    )
  | `Case6(
      null
    )
  ]

and object_ =
  (
      string /* "{" */
    ,
      [
      | `Case0(
          (
              pair
            ,
              list( /* zero or more */
                (
                    string /* "," */
                  ,
                    pair
                )
              )
          )
        )
      | `Case1(
          string /* blank */
        )
      ]
    ,
      string /* "}" */
  )

and pair =
  (
      [
      | `Case0(
          string
        )
      | `Case1(
          number
        )
      ]
    ,
      string /* ":" */
    ,
      _value
  )

and array =
  (
      string /* "[" */
    ,
      [
      | `Case0(
          (
              _value
            ,
              list( /* zero or more */
                (
                    string /* "," */
                  ,
                    _value
                )
              )
          )
        )
      | `Case1(
          string /* blank */
        )
      ]
    ,
      string /* "]" */
  )

and string =
  [
  | `Case0(
      (
          string /* "\"" */
        ,
          string /* "\"" */
      )
    )
  | `Case1(
      (
          string /* "\"" */
        ,
          string_content
        ,
          string /* "\"" */
      )
    )
  ]

and string_content =
  list( /* one or more */
    [
    | `Case0(
        string /* pattern "[^\\\\\"\\n]+" */
      )
    | `Case1(
        escape_sequence
      )
    ]
  )

and escape_sequence =
  (
      string /* "\\" */
    ,
      string /* pattern "(\\\"|\\\\|\\/|b|n|r|t|u)" */
  )

and number =
  [
  | `Case0(
      (
          [
          | `Case0(
              string /* "0x" */
            )
          | `Case1(
              string /* "0X" */
            )
          ]
        ,
          string /* pattern "[\\da-fA-F]+" */
      )
    )
  | `Case1(
      (
          [
          | `Case0(
              string /* "-" */
            )
          | `Case1(
              string /* "+" */
            )
          | `Case2(
              string /* blank */
            )
          ]
        ,
          [
          | `Case0(
              string /* "0" */
            )
          | `Case1(
              (
                  string /* pattern "[1-9]" */
                ,
                  [
                  | `Case0(
                      string /* pattern "\\d+" */
                    )
                  | `Case1(
                      string /* blank */
                    )
                  ]
              )
            )
          ]
        ,
          string /* "." */
        ,
          [
          | `Case0(
              string /* pattern "\\d+" */
            )
          | `Case1(
              string /* blank */
            )
          ]
        ,
          [
          | `Case0(
              (
                  [
                  | `Case0(
                      string /* "e" */
                    )
                  | `Case1(
                      string /* "E" */
                    )
                  ]
                ,
                  [
                  | `Case0(
                      string /* "-" */
                    )
                  | `Case1(
                      string /* "+" */
                    )
                  | `Case2(
                      string /* blank */
                    )
                  ]
                ,
                  string /* pattern "\\d+" */
              )
            )
          | `Case1(
              string /* blank */
            )
          ]
      )
    )
  | `Case2(
      (
          string /* "." */
        ,
          string /* pattern "\\d+" */
        ,
          [
          | `Case0(
              (
                  [
                  | `Case0(
                      string /* "e" */
                    )
                  | `Case1(
                      string /* "E" */
                    )
                  ]
                ,
                  [
                  | `Case0(
                      string /* "-" */
                    )
                  | `Case1(
                      string /* "+" */
                    )
                  | `Case2(
                      string /* blank */
                    )
                  ]
                ,
                  string /* pattern "\\d+" */
              )
            )
          | `Case1(
              string /* blank */
            )
          ]
      )
    )
  | `Case3(
      (
          [
          | `Case0(
              string /* "-" */
            )
          | `Case1(
              string /* "+" */
            )
          | `Case2(
              string /* blank */
            )
          ]
        ,
          [
          | `Case0(
              string /* "0" */
            )
          | `Case1(
              (
                  string /* pattern "[1-9]" */
                ,
                  [
                  | `Case0(
                      string /* pattern "\\d+" */
                    )
                  | `Case1(
                      string /* blank */
                    )
                  ]
              )
            )
          ]
        ,
          [
          | `Case0(
              (
                  [
                  | `Case0(
                      string /* "e" */
                    )
                  | `Case1(
                      string /* "E" */
                    )
                  ]
                ,
                  [
                  | `Case0(
                      string /* "-" */
                    )
                  | `Case1(
                      string /* "+" */
                    )
                  | `Case2(
                      string /* blank */
                    )
                  ]
                ,
                  string /* pattern "\\d+" */
              )
            )
          | `Case1(
              string /* blank */
            )
          ]
      )
    )
  | `Case4(
      (
          [
          | `Case0(
              string /* "0b" */
            )
          | `Case1(
              string /* "0B" */
            )
          ]
        ,
          string /* pattern "[0-1]+" */
      )
    )
  | `Case5(
      (
          [
          | `Case0(
              string /* "0o" */
            )
          | `Case1(
              string /* "0O" */
            )
          ]
        ,
          string /* pattern "[0-7]+" */
      )
    )
  ]

and true_ =
  string /* "true" */

and false_ =
  string /* "false" */

and null =
  string /* "null" */
;
